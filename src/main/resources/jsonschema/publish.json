{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "subjectId": {
      "type": "number"
    },
    "authorsContributors": {
      "type": "string"
    },
    "subjectLessonTypeId": {
      "type": "number"
    },
    "subjectLessonLevelId": {
      "type": "number"
    },
    "subjectTagList": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "id": {
            "type": "number"
          },
          "label": {
            "type": "string"
          }
        },
        "required": ["label"]
      }
    }
  },
  "required": [
    "subjectId",
    "subjectLessonTypeId",
    "subjectLessonLevelId"
  ]
}